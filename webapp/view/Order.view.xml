<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ZEFORM.PettyCash.controller.Order"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:u="sap.ui.unified"
	xmlns:t="sap.ui.table" xmlns:smarttable="sap.ui.comp.smarttable" xmlns:smartfilterbar="sap.ui.comp.smartfilterbar">
	<App>
		<pages>
			<Page title="Title" showHeader="false">
				<footer>
					<Toolbar>
						<ToolbarSpacer/>
						<Button icon="sap-icon://create-form" id="createBtn" press="onCreate" tooltip="Create"/>
						<Button icon="sap-icon://copy" id="copyBtn" press="onCopy" tooltip="Copy"/>
						<Button icon="sap-icon://save" id="saveBtn" press="onSave" tooltip="Save"/>
						<Button icon="sap-icon://add-document" id="uploadBtn" press="openDocDialog" tooltip="Upload Document(s)"/>
						<Button icon="sap-icon://detail-view" id="previewBtn" press="onPreview" tooltip="Preview PC Claim"/>
						<Button icon="sap-icon://print" id="printBtn" press="onPrint" tooltip="Print PC Claim"/>
						<Button icon="sap-icon://request" id="changeLogBtn" press="openLogDialog" tooltip="Change Log"/>
						<Button icon="sap-icon://delete" id="deleteBtn" press="onDelete" tooltip="Delete"/>
						<Button icon="sap-icon://payment-approval" id="reimburseBtn" press="onReimburse" tooltip="Reimburse"/>
						<Button icon="sap-icon://switch-classes" id="reassignBtn" press="openPrepareDialog" tooltip="Reassign Preparer"/>
					</Toolbar>
				</footer>
				<content>
					<!--Start of Header-->
					<ObjectHeader backgroundDesign="Translucent" fullScreenOptimized="false" responsive="true" titleLevel="H6" condensed="true"
						titleActive="true" titlePress="handleTitlePress" busyIndicatorDelay="0" title="">
						<ObjectAttribute title="Claim No." text="{PC>/Claimno}"></ObjectAttribute>
						<ObjectAttribute title="Batch No." text="{PC>/Batchno}"/>
						<ObjectAttribute title="Created By" text="{PC>/PreparerName}"/>
						<ObjectAttribute title="Created On" text="{path: 'PC>/CrtDate', formatter:'.formatter.formatDate'}"/>
						<!--<ObjectAttribute title="Posting Date" text="{PC>/Cusapbudat}"/>-->
						<statuses>
							<ObjectStatus title="Status" text="{PC>/Statustext}"/>
							<ObjectStatus title="CUSAP Doc. No." text="{PC>/Cusapbelnr}"/>
							<!--<ObjectStatus title="" id="forwardedPerson"/>-->
						</statuses>
					</ObjectHeader>
					<f:SimpleForm editable="true" layout="ResponsiveGridLayout">
						<FormattedText
							htmlText="&lt;span style='text-align:left;font-weight:bold'&gt;*** Changes must be saved first before they will take effect for your next action.&lt;/span&gt;">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</FormattedText>
					</f:SimpleForm>
					<f:Form editable="true">
						<f:layout>
							<f:ResponsiveGridLayout labelSpanXL="5" labelSpanL="5" labelSpanM="5" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0"
								emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false"/>
						</f:layout>
						<f:formContainers>
							<f:FormContainer>
								<f:formElements>
									<f:FormElement label="Float Admin Party">
										<f:fields>
											<Input type="Text" value="{PC>/Admin}" enabled="{PC>/AdminEnabled}" change="checkAdminParty" required="true" maxLength="24"
												tooltip="{i18n>faptooltip}"/>
										</f:fields>
									</f:FormElement>
									<f:FormElement label="Department">
										<f:fields>
											<Input type="Text" value="{PC>/PcDept}" enabled="{PC>/PcDeptEnabled}" required="true" maxLength="70"/>
										</f:fields>
									</f:FormElement>
									<f:FormElement label="Division/Unit">
										<f:fields>
											<Input type="Text" value="{PC>/Division}" enabled="{PC>/DivisionEnabled}" maxLength="70"/>
										</f:fields>
									</f:FormElement>
									<f:FormElement label="Claimant Name">
										<f:fields>
											<Input id="CtEmail1" type="Text" value="{PC>/ClaimName}" enabled="{PC>/ClaimNameEnabled}" maxLength="70"
												tooltip="Email address of the contact person."/>
										</f:fields>
									</f:FormElement>
									<f:FormElement label="Claimant Post">
										<f:fields>
											<Input type="Text" value="{PC>/ClaimPost}" enabled="{PC>/ClaimPostEnabled}" maxLength="70" tooltip="Telephone number of the contact person."/>
										</f:fields>
									</f:FormElement>
									<f:FormElement label="Claims Date Range (From / To)">
										<f:fields>
											<DatePicker id="claimDatfrDatePicker" displayFormat="dd.MM.yyyy" change="changeClaimDatfr" dateValue="{PC>/ClaimDatfr}"
												enabled="{PC>/ClaimDatfrEnabled}"/>
											<DatePicker id="claimDattoDatePicker" displayFormat="dd.MM.yyyy" change="changeClaimDatto" dateValue="{PC>/ClaimDatto}"
												enabled="{PC>/ClaimDattoEnabled}"/>
										</f:fields>
									</f:FormElement>
									<f:FormElement label="Contact Phone No.">
										<f:fields>
											<Input id="ContPhno" type="Text" value="{PC>/ContPhno}" maxLength="35" tooltip="Telephone number of the contact person."
												enabled="{PC>/ContPhnoEnabled}"/>
										</f:fields>
									</f:FormElement>
									<f:FormElement label="Header Reference">
										<f:fields>
											<Button text="Reference Fields" type="Emphasized" press="openHeadReferenceDialog"/>
										</f:fields>
									</f:FormElement>
								</f:formElements>
							</f:FormContainer>
						</f:formContainers>
					</f:Form>
					<!--<Text class="sLabel" text="Other Information:"></Text>-->
					<!--End of Header-->
					<!--Start of Accout Assignments-->
					<Toolbar class="itemLabel">
						<content>
							<Title class="buttonLabel2" text="Petty Cash Claims" width="300px"/>
							<Button class="buttonLabel" icon="sap-icon://add" press="itemAddRow" id="itemAddRowBtn" tooltip="add"/>
							<Button class="buttonLabel" icon="sap-icon://less" press="itemRemoveRow" id="itemRemoveRowBtn" tooltip="less"/>
							<Button class="buttonLabel" icon="sap-icon://copy" press="itemCopyRow" id="itemCopyRowBtn"
								tooltip="The selected line item, including its account assignment(s), can be copied."/>
						</content>
					</Toolbar>
					<ScrollContainer horizontal="true" focusable="true">
						<Table id="itemTable1" updateFinished="refreshTotal" mode="MultiSelect"
							items="{path: 'PC>/PCHEADTOITEM',length:1000,templateShareable:false}">
							<columns>
								<Column width="6rem">
									<Label text="Item No." required="true"/>
								</Column>
								<Column width="5rem">
									<Text text="Curr."/>
								</Column>
								<Column width="6rem">
									<Label text="Amount" required="true"/>
									<footer>
										<Text class="sapMInputBaseInner" textAlign="Right"
											text="{path: 'PC>/Tolamt', type:'sap.ui.model.type.Float', formatOptions: { minFractionDigits: 2, maxFractionDigits: 2 }}"
											tooltip="The total amount of all line items under Charging Information (Debit)."/>
									</footer>
								</Column>
								<Column width="7rem">
									<Text text="Company"/>
								</Column>
								<Column width="8rem">
									<Label text="Business Area"/>
								</Column>
								<Column width="6rem">
									<Label text="Cost Centre"/>
								</Column>
								<Column width="6rem">
									<Text text="Project"/>
								</Column>
								<Column width="8rem">
									<Text text="Internal Order"/>
								</Column>
								<Column width="7rem">
									<Label text="A/C Code" required="true"/>
								</Column>
								<Column width="8rem">
									<Text text="Category"/>
								</Column>
								<Column width="7rem" visible="{PC>/AssignmentVisible}">
									<Text text="Assignment"/>
								</Column>
								<Column width="9rem">
									<Text text="Budget Holder"/>
								</Column>
								<Column width="9rem">
									<Text text="Remarks(Optional)"/>
								</Column>
								<Column width="10rem"/>
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<Input value="{path:'PC>Claimitm'}" id="accitem" enabled="{PC>ClaimitmEnabled}" maxLength="5" change="changeItemId"
											tooltip="This is simply a sequence number assigned to each individual item listed in a petty cash claim."/>
										<!-- Item No -->
										<Input value="{PC>Waers}" enabled="false"/>
										<!-- Curr. -->
										<Input value="{path: 'PC>Amount', type:'sap.ui.model.type.Float', formatOptions: { minFractionDigits: 2, maxFractionDigits: 2 }}"
											enabled="{PC>AmountEnabled}" textAlign="Right" change="changeAmount" tooltip="The amount of this claim item."/>
										<!-- Amount -->
										<Input value="{PC>Bukrs}" enabled="false" change="getBudgetHolder" maxLength="4"
											tooltip="Company Code is automatically defaulted according to cost centre or project."/>
										<!-- Company -->
										<Input value="{PC>Gsber}" enabled="{PC>GsberEnabled}" change="getBudgetHolder" showValueHelp="true" valueHelpRequest="businessAreaValueHelp"
											valueHelpOnly="false" tooltip="If business area is not input, it would be defaulted according to cost centre or project."/>
										<!-- Business Area -->
										<Input value="{PC>Kostl}" enabled="{PC>KostlEnabled}" maxLength="10" tooltip="Either cost centre or project should be entered."
											change="getBudgetHolder"/>
										<!-- Cost Centre -->
										<Input value="{PC>Projn}" enabled="{PC>ProjnEnabled}" change="getBudgetHolder" tooltip="Either cost centre or project should be entered."
											maxLength="24"/>
										<!-- Project -->
										<Input value="{PC>Aufnr}" maxLength="12" valueHelpRequest="aufnrValueHelp" valueHelpOnly="false"
											tooltip="Internal order can be used for sub-allocation within department." enabled="{PC>AufnrEnabled}" showValueHelp="true"
											change="getBudgetHolder"/>
										<!-- Internal Order -->
										<Input value="{PC>Sakto}" enabled="{PC>SaktoEnabled}" showValueHelp="true" valueHelpRequest="acCodeValueHelp" required="true"
											tooltip="Account code to be debited for this claim item. Please input the account code manually if it is not in the search list."
											maxLength="10" change="changeCategory"/>
										<!-- A/C Code -->
										<Input value="{PC>Category}" enabled="false" maxLength="50" tooltip="Category is automatically defaulted according to A/C Code."/>
										<!-- Category -->
										<Input value="{PC>Assignment}" maxLength="18" enabled="{PC>AssignmentEnabled}"/>
										<!-- Assignment -->
										<Input value="{PC>Name1}" enabled="false" maxLength="70"
											tooltip="Budget Holder is automatically defaulted according to cost centre / project / internal order."/>
										<!-- Budget Holder -->
										<Input value="{PC>Remarks}" maxLength="254" enabled="{PC>RemarksEnabled}"/>
										<!-- Remarks(Optional) -->
										<Button text="Ref.Field" width="100%" type="Emphasized" enabled="{PC>RefEnabled}" press="openAccRefDialog"></Button>
										<!-- Ref btn -->
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</ScrollContainer>
					<f:SimpleForm editable="true" layout="ResponsiveGridLayout">
						<Label text="Remarks for Department's Internal Use">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</Label>
						<TextArea growing="true" rows="1" value="{PC>/Rdpiu}" enabled="{PC>/RdpiuEnabled}" maxLength="254"
							tooltip="Remarks for department/unit’s internal reference only. These remarks will not be shown on the printout of petty cash claim nor replenishment request.">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</TextArea>
						<Label text="Remarks for FNO's Information">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</Label>
						<TextArea growing="true" rows="1" value="{PC>/Remarkinfofo}" enabled="{PC>/RemarkinfofoEnabled}" maxLength="254"
							tooltip="Department can input a message in this field for Finance Office’s information. These remarks will be shown on the printout of petty cash replenishment request.">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</TextArea>
						<Label text="Remarks for FNO's Internal Use">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</Label>
						<TextArea growing="true" rows="1" value="{PC>/RemarkFo}" enabled="{PC>/RemarkFoEnabled}" visible="{PC>/RemarkFoVisible}" maxLength="254"
							tooltip="Remarks for Finance Office’s internal reference only.">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</TextArea>
						<Label text="Message to Department">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</Label>
						<TextArea growing="true" rows="1" value="{PC>/Message}" enabled="{PC>/MessageEnabled}" maxLength="254"
							tooltip="This field can be used by the Finance Office (FNO) to record any subsequent remarks about the claim/batch after the batch is rejected/approved by FNO or is posted to CUSAP (i.e. when other e-form fields can no longer be edited by FNO).">
							<layoutData>
								<l:GridData span="XL12 L12 M12 S12"/>
							</layoutData>
						</TextArea>
					</f:SimpleForm>
				</content>
			</Page>
			<Page>
				<BusyDialog id="loadingDialog" title="Loading"/>
				<Dialog id="docDialog" title="Supporting Document" draggable="true" contentWidth="70%">
					<content>
						<Toolbar>
							<u:FileUploader id="docFile" uploadUrl="../upload" sameFilenameAllowed="true"/>
							<Button text="Upload" press="docAddRow" id="uploadDocBtn"/>
							<Button text="Delete" press="docRemoveRow" id="removeDocBtn"/>
						</Toolbar>
						<Table id="docTable" fixedLayout="true" mode="MultiSelect" items="{path: 'PC>/PCHEADTODOCTemp'}">
							<columns>
								<Column width="15rem">
									<Text text="File Name"/>
								</Column>
								<Column width="8rem">
									<Text text="Upload Date"/>
								</Column>
								<Column width="8rem">
									<Text text="FNO"/>
								</Column>
								<Column width="8rem">
									<Text text="Dept."/>
								</Column>
								<Column width="11rem">
									<Text text="Parent Folder"/>
								</Column>
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<Link text="{PC>Fname}" press="downloadDoc" tooltip="{PC>Fname}"/>
										<Text text="{path: 'PC>Updat', formatter:'.formatter.formatDate'}"/>
										<Text text="{PC>Fno}"/>
										<Text text="{PC>Dept}"/>
										<Text text="{PC>Parent}"/>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</content>
					<buttons>
						<Button text="Save" press="saveDocument"/>
						<Button text="Cancel" press="closeDocDialog"/>
					</buttons>
				</Dialog>
				<Dialog id="refDialog" title="Header Reference Field" contentWidth="650px">
					<content>
						<l:Grid class="sapUiSmallMargin" defaultSpan="XL12 L12 M12 S12">
							<VBox>
								<layoutData >
									<l:GridData span="XL12 L12 M12 S12"/>
								</layoutData>
								<f:SimpleForm editable="true" layout="ResponsiveGridLayout">
									<Label text="Department Reference" visible="{PC>/REF/Reference6Visible}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Label>
									<Input value="{PC>/REF/Reference6}" enabled="{PC>/REF/Reference6Enabled}" visible="{PC>/REF/Reference6Visible}" id="reference6"
										maxLength="50" tooltip="{i18n>internalUseTooltip}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Input>
									<Label text="Service/Section/Unit/Category " visible="{PC>/REF/Reference7Visible}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Label>
									<Select items="{ path: 'PC>/SECTIONSet'}" selectedKey="{PC>/REF/Reference7}" visible="{PC>/REF/Reference7Visible}"
										enabled="{PC>/REF/Reference7Enabled}" id="reference7" showSecondaryValues="true" forceSelection="false" tooltip="{i18n>internalUseTooltip}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
										<core:ListItem key="{PC>SectCode}" text="{PC>SectCode}" additionalText="{PC>Description}"/>
									</Select>
									<Label text="Project/Research Reference" visible="{PC>/REF/Reference8Visible}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Label>
									<Input value="{PC>/REF/Reference8}" enabled="{PC>/REF/Reference8Enabled}" visible="{PC>/REF/Reference8Visible}" id="reference8"
										maxLength="50" tooltip="{i18n>internalUseTooltip}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Input>
									<Label text="Reference 1">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Label>
									<Input value="{PC>/REF/Reference1}" maxLength="50" enabled="{PC>/REF/Reference1Enabled}" id="reference1" tooltip="{i18n>internalUseTooltip}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Input>
									<Label text="Reference 2">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Label>
									<Input value="{PC>/REF/Reference2}" maxLength="50" enabled="{PC>/REF/Reference2Enabled}" id="reference2" tooltip="{i18n>internalUseTooltip}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Input>
									<Label text="Reference 3">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Label>
									<Input value="{PC>/REF/Reference3}" maxLength="50" enabled="{PC>/REF/Reference3Enabled}" id="reference3" tooltip="{i18n>internalUseTooltip}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Input>
									<Label text="Reference 4">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Label>
									<Input value="{PC>/REF/Reference4}" maxLength="50" enabled="{PC>/REF/Reference4Enabled}" id="reference4" tooltip="{i18n>internalUseTooltip}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Input>
									<Label text="Reference 5">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Label>
									<Input value="{PC>/REF/Reference5}" maxLength="50" enabled="{PC>/REF/Reference5Enabled}" id="reference5" tooltip="{i18n>internalUseTooltip}">
										<layoutData>
											<l:GridData span="XL6 L6 M6 S6"/>
										</layoutData>
									</Input>
								</f:SimpleForm>
							</VBox>
						</l:Grid>
					</content>
					<buttons>
						<Button text="OK" press="onRef"/>
						<Button text="Cancel" press="closeHeadReferenceDialog"/>
					</buttons>
				</Dialog>
				<Dialog id="accRefDialog" title="Item Reference Field " contentWidth="600px">
					<Toolbar>
						<Title id="accReferenceTitle"/>
						<Title id="accReferenceDetail"/>
					</Toolbar>
					<content>
						<l:Grid class="sapUiSmallMargin" defaultSpan="XL12 L12 M12 S12">
							<VBox>
								<f:SimpleForm editable="true" layout="ResponsiveGridLayout">
									<Label text="Product/Service Type">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/ProductType}" maxLength="30" enabled="{PC>/accREF/ProductTypeEnabled}" id="item_reference" showValueHelp="true"
										valueHelpRequest="productTypeValueHelp" valueHelpOnly="true">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 1">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference1}" maxLength="50" enabled="{PC>/accREF/Reference1Enabled}" id="acc_reference1"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 2">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference2}" maxLength="50" enabled="{PC>/accREF/Reference2Enabled}" id="acc_reference2"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 3">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference3}" maxLength="50" enabled="{PC>/accREF/Reference3Enabled}" id="acc_reference3"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 4">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference4}" maxLength="50" enabled="{PC>/accREF/Reference4Enabled}" id="acc_reference4"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 5">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference5}" maxLength="50" enabled="{PC>/accREF/Reference5Enabled}" id="acc_reference5"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 6">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference6}" maxLength="50" enabled="{PC>/accREF/Reference5Enabled}" id="acc_reference6"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 7">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference7}" maxLength="50" enabled="{PC>/accREF/Reference5Enabled}" id="acc_reference7"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 8">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference8}" maxLength="50" enabled="{PC>/accREF/Reference5Enabled}" id="acc_reference8"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 9">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference9}" maxLength="50" enabled="{PC>/accREF/Reference5Enabled}" id="acc_reference9"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
									<Label text="Reference 10">
										<layoutData>
											<l:GridData span="XL4 L4 M4 S4"/>
										</layoutData>
									</Label>
									<Input value="{PC>/accREF/Reference10}" maxLength="50" enabled="{PC>/accREF/Reference5Enabled}" id="acc_reference10"
										tooltip="Reference field, for internal use only.">
										<layoutData>
											<l:GridData span="XL8 L8 M8 S8"/>
										</layoutData>
									</Input>
								</f:SimpleForm>
